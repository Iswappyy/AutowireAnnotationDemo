<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />
	<bean id="computer" class="com.prowings.autowireannotation.Computer">
		<property name="compId" value="4010"></property>
		<property name="modelName" value="DELL"></property>

	</bean>
	
    <bean id="cpu" class="com.prowings.autowireannotation.Central_processor">
		<property name="processor_name" value="IntelCore"></property>
		<property name="processor_ram" value="32"></property>
		<property name="processor_rom" value="512"></property>


	</bean>
	<bean id="cpu1" class="com.prowings.autowireannotation.Central_processor">
		<property name="processor_name" value="AMWCore"></property>
		<property name="processor_ram" value="16"></property>
		<property name="processor_rom" value="270"></property>
	</bean>
</beans>